<!-- TinyHAL configuration file for Samsung Galaxy S III -->
<tinyhal>
<!--
We are able to have most of our routing static so do that
-->
<path>
    <!-- stdac on -->
    <ctl name="CPCAP Mixer Stereo DAC" val="1"/>

    <ctl name="Ext Volume" val="12"/>
    <ctl name="Codec Volume" val="12"/>
    <ctl name="STDac Volume" val="12"/>

    <ctl name="MIC1 Gain" val="0"/>
    <ctl name="MIC2 Gain" val="0"/>

    <ctl name="Analog External Left Capture Route" val="0"/>
    <ctl name="Analog External Right Capture Route" val="0"/>
    <ctl name="Analog Left" val="Mic2"/>
    <ctl name="Analog Right" val="Mic1"/>

</path>
<device name="speaker">
    <path name="on">
        <ctl name="LDSPLEXT Switch" val="1"/>
	<ctl name="LDSPREXT Switch" val="1"/>
        <ctl name="LDSPLDAC Switch" val="1"/>
	<ctl name="LDSPRDAC Switch" val="1"/>
        <ctl name="LDSPLCDC Switch" val="1"/>
	<ctl name="LDSPRCDC Switch" val="1"/>
    </path>
    <path name="off">
        <ctl name="LDSPLEXT Switch" val="0"/>
	<ctl name="LDSPREXT Switch" val="0"/>
        <ctl name="LDSPLDAC Switch" val="0"/>
	<ctl name="LDSPRDAC Switch" val="0"/>
        <ctl name="LDSPLCDC Switch" val="0"/>
	<ctl name="LDSPRCDC Switch" val="0"/>
    </path>
</device>
<device name="earpiece">
    <path name="on">
        <ctl name="EPEXT" val="1"/>
	<ctl name="EPCDC" val="1"/>
	<ctl name="EPDAC" val="1"/>
    </path>
    <path name="off">
	<ctl name="EPEXT" val="0"/>
	<ctl name="EPCDC" val="0"/>
	<ctl name="EPDAC" val="0"/>
    </path>
</device>
<device name="headphone">
    <path name="on">
        <ctl name="HSLEXT" val="1"/>
	<ctl name="HSLCDC" val="1"/>
	<ctl name="HSLDAC" val="1"/>
        <ctl name="HSREXT" val="1"/>
	<ctl name="HSRCDC" val="1"/>
	<ctl name="HSRDAC" val="1"/>
    </path>
    <path name="off">
        <ctl name="HSLEXT" val="0"/>
	<ctl name="HSLCDC" val="0"/>
	<ctl name="HSLDAC" val="0"/>
        <ctl name="HSREXT" val="0"/>
	<ctl name="HSRCDC" val="0"/>
	<ctl name="HSRDAC" val="0"/>
	 </path>
</device>
<device name="sco-out">
    <path name="on">
    </path>
    <path name="off">
    </path>
</device>
<device name="analog-dock">
    <path name="on">
    </path>
    <path name="off">
    </path>
</device>
<device name="digital-dock">
    <path name="on">
    </path>
    <path name="off">
    </path>
</device>
<device name="builtin-mic">
    <path name="on">
    <ctl name="Analog Left Capture Route" val="Mic2"/>
    <ctl name="Analog Right Capture Route" val="Mic1"/>
    <ctl name="MIC1 Gain" val="31"/>
    <ctl name="MIC2 Gain" val="31"/>
    </path>
    <path name="off">
    <ctl name="MIC1 Gain" val="0"/>
    <ctl name="MIC2 Gain" val="0"/>
   <ctl name="Analog Left Capture Route" val="Off"/>
    <ctl name="Analog Right Capture Route" val="Off"/>
    </path>
</device>
<device name="back-mic">
    <path name="on">
    <ctl name="Analog Left Capture Route" val="Mic2"/>
    <ctl name="Analog Right Capture Route" val="Mic1"/>
    <ctl name="MIC1 Gain" val="31"/>
    <ctl name="MIC2 Gain" val="31"/>
    </path>
    <path name="off">
    <ctl name="MIC1 Gain" val="0"/>
    <ctl name="MIC2 Gain" val="0"/>
   <ctl name="Analog Left Capture Route" val="Off"/>
    <ctl name="Analog Right Capture Route" val="Off"/>
    </path>
</device>
<device name="headset-in">
    <path name="on">
    <ctl name="MIC1 Gain" val="31"/>
    <ctl name="MIC2 Gain" val="31"/>
    <ctl name="Analog External Left Capture Route" val="EXT MicL"/>
    <ctl name="Analog External Right Capture Route" val="EXT MicR"/>
    </path>
    <path name="off">
    <ctl name="MIC1 Gain" val="0"/>
    <ctl name="MIC2 Gain" val="0"/>
    <ctl name="Analog External Left Capture Route" val="Off"/>
    <ctl name="Analog External Right Capture Route" val="Off"/>
    </path>
</device>
<device name="sco-in">
    <path name="on">
    </path>
    <path name="off">
    </path>
</device>
<device name="fmradio">
    <path name="on">
    </path>
    <path name="off">
    </path>
</device>
</tinyhal>

